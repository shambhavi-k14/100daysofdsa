#include <stdio.h>

int main() {
    int n, k, i;

    // Input size
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    // Input array elements
    printf("Enter elements:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Input number of rotations
    printf("Enter value of k: ");
    scanf("%d", &k);

    // Handle k greater than n
    k = k % n;

    // Rotate array k times
    for(int r = 0; r < k; r++) {
        int last = arr[n - 1];

        for(i = n - 1; i > 0; i--) {
            arr[i] = arr[i - 1];
        }

        arr[0] = last;
    }

    // Print rotated array
    printf("Array after right rotation:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
