#include <stdio.h>

int main() {
    int n, k, i;

    // Input array size
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    // Input elements
    printf("Enter elements:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Input k
    printf("Enter number of rotations (k): ");
    scanf("%d", &k);

    // If k > n
    k = k % n;

    // Right rotation
    while(k--) {
        int last = arr[n - 1];

        for(i = n - 1; i > 0; i--) {
            arr[i] = arr[i - 1];
        }

        arr[0] = last;
    }

    // Display rotated array
    printf("Array after rotation:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
